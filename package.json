{
  "name": "gatsby-starter-prismic-typescript",
  "version": "0.1.0",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby and Prismic using TypeScript",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "gen": "graphql-codegen --config graphql-codegen.yml",
    "format": "prettier --write \"**/*.{js,tsx,ts,tsx,json,md}\""
  },
  "license": "MIT",
  "author": "Angelo Ashmore",
  "homepage": "https://github.com/angeloashmore/gatsby-source-prismic/tree/master/starters/gatsby-starter-prismic",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/gatsby-source-prismic"
  },
  "bugs": {
    "url": "https://github.com/angeloashmore/gatsby-source-prismic/issues"
  },
  "keywords": [
    "gatsby"
  ],
  "dependencies": {
    "gatsby": "^3.13.0",
    "gatsby-plugin-catch-links": "^3.13.0",
    "gatsby-plugin-image": "^1.13.0",
    "gatsby-plugin-prismic-previews": "^4.1.3",
    "gatsby-plugin-react-helmet-async": "^1.2.0",
    "gatsby-source-prismic": "^4.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.1.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/typescript-operations": "^2.1.4",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "dotenv": "^10.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.4.0"
  }
}
